<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

	  <managed-bean>
		<managed-bean-name>ingreso</managed-bean-name>
		<managed-bean-class>proyecto.ctrl.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
        <managed-bean>
		<managed-bean-name>mantUsr</managed-bean-name>
		<managed-bean-class>proyecto.mant.MantUsuario</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
        <managed-bean>
		<managed-bean-name>mantClientes</managed-bean-name>
		<managed-bean-class>proyecto.mant.MantClientes</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
       
        <managed-bean>
		<managed-bean-name>operDispo</managed-bean-name>
		<managed-bean-class>proyecto.oper.CtrlDisponible</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
        </managed-bean>
        <managed-bean>
		<managed-bean-name>operReserv</managed-bean-name>
		<managed-bean-class>proyecto.oper.opReservar</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
        </managed-bean>
        
        
</faces-config>
